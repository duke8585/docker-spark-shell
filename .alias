function ss313() {
    loc="spark_3_1_3"
	name="local_$loc"
	docker rm -f $name
	cd ~/devel/docker-spark-shell/$loc/
    docker build -t $name .
	docker run -it \
	--name $name \
	--volume ~/sandbox:/cwd \
	-p 5050:4040 \
	$name \
	bash -c "printf ðŸ›‘ http://localhost:5050 for history ðŸ›‘ && bash"
	cd -
}

